local Settings = Library:Tab("การตั้งค่า")

Main3:Label("Welcome "..game.Players.LocalPlayer.DisplayName.. " To Silver Hub")

Main3:Label("By loui#4370")

local Time = Main3:Label("servertime")

function UpdateTime()
    local GameTime = math.floor(workspace.DistributedGameTime + 0.5)
    local Hour = math.floor(GameTime / (60 ^ 2)) % 24
    local Minute = math.floor(GameTime / (60 ^ 1)) % 60
    local Second = math.floor(GameTime / (60 ^ 0)) % 60
    Time:Set("Hour : " .. Hour .. " Minute : " .. Minute .. " Second : " .. Second)
end

spawn(function()
    while true do
        UpdateTime()
        wait()
    end
end)

local FPS = Main3:Label("FPS Player")

function UpdateFps()
    local Ping = math.floor(tonumber(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()))
    local Fps = math.floor(workspace:GetRealPhysicsFPS())
    FPS:Set("Fps : " .. Fps .. " Ping : " .. Ping)
end

spawn(function()
    while true do
        wait(0.1)
        UpdateFps()
    end
end)

