    Main:Seperator("Bone")



    local BoneFarm = Main:Toggle("Auto Farm Bone",_G.Auto_Bone,function(value)

        _G.Auto_Bone = value

    StopTween(_G.Auto_Bone)

    end)









    spawn(function()

        while wait() do 

            if _G.Auto_Bone and W3 then

                pcall(function()



                                        if game:GetService("Workspace").Enemies:FindFirstChild("Reborn Skeleton") or game:GetService("Workspace").Enemies:FindFirstChild("Living Zombie") or game:GetService("Workspace").Enemies:FindFirstChild("Demonic Soul") or game:GetService("Workspace").Enemies:FindFirstChild("Posessed Mummy") then

                        for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do

                            if v.Name == "Reborn Skeleton" or v.Name == "Living Zombie" or v.Name == "Demonic Soul" or v.Name == "Posessed Mummy" then

                                if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then

                                    repeat task.wait()

                                        AutoHaki()

                                        EquipWeapon(_G.SelectWeapon)



                                        v.HumanoidRootPart.CanCollide = false

                                        v.Humanoid.WalkSpeed = 0    



v.Head.CanCollide = false 

                                        StartMagnetBoneMon = true

                                        PosMonBone = v.HumanoidRootPart.CFrame







topos(v.HumanoidRootPart.CFrame * CFrame.new(0,10,20))





                                    until not 

     _G.Auto_Bone or not v.Parent or v.Humanoid.Health <= 0



                                            end

                                        end

                                    end

                                else

                                    MagnetDought = false

                                    if game:GetService("ReplicatedStorage"):FindFirstChild("Reborn Skeleton") then

                                        topos(game:GetService("ReplicatedStorage"):FindFirstChild("Reborn Skeleton").HumanoidRootPart.CFrame * CFrame.new(0,0,10)) 

                                    else

                      if not

                        game:GetService("ReplicatedStorage"):FindFirstChild("Reborn Skeleton") then

                        topos(CFrame.new(-9515.3720703125, 163.9804229736328, 5786.06103515625))



                                    else

                                        if game:GetService("ReplicatedStorage"):FindFirstChild("Living Zombie") then

                                            topos(game:GetService("ReplicatedStorage"):FindFirstChild("Living Zombie").HumanoidRootPart.CFrame * CFrame.new(0,0,10)) 

                                        else

                                            if game:GetService("ReplicatedStorage"):FindFirstChild("Demonic Soul") then

                                                topos(game:GetService("ReplicatedStorage"):FindFirstChild("Demonic Soul").HumanoidRootPart.CFrame * CFrame.new(0,0,10))

                                            else

                                                if game:GetService("ReplicatedStorage"):FindFirstChild("Posessed Mummy") then

                                                    topos(game:GetService("ReplicatedStorage"):FindFirstChild("Posessed Mummy").HumanoidRootPart.CFrame * CFrame.new(0,0,10))

                        end

                      end

                    end

                  end

                end

              end

            end)

          end

        end

      end)





