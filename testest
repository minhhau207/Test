                for i,v in pairs(game:GetService("Players"):GetPlayers()) do

                    if i == 12 then

                        plyserv:Set("Players :".." "..i.." ".."/".." ".."12".." ".."(Max)")

                    elseif i == 1 then

                        plyserv:Set("Player :".." "..i.." ".."/".." ".."12")

                    else

                        plyserv:Set("Players :".." "..i.." ".."/".." ".."12")

                    end

                end

            end)

        end

    end)



