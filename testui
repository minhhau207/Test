                                    ) /
                                        mq.AbsoluteSize.Y
                                mq.InputBegan:Connect(
                                    function(iq)
                                        if iq.UserInputType == Enum.UserInputType.MouseButton1 then
                                            if Aq then
                                                Aq:Disconnect()
                                            end
                                            M = true
                                            Aq =
                                                Oq.RenderStepped:Connect(
                                                function()
                                                    local Nq =
                                                        (math.clamp(uq.X - mq.AbsolutePosition.X, 0, mq.AbsoluteSize.X) /
                                                        mq.AbsoluteSize.X)
                                                    local Gq =
                                                        (math.clamp(uq.Y - mq.AbsolutePosition.Y, 0, mq.AbsoluteSize.Y) /
                                                        mq.AbsoluteSize.Y)
                                                    cq.Position = UDim2.fromScale(Nq, Gq)
                                                    E7 = Nq
                                                    l7 = 1 - Gq
                                                    zq()
                                                end
                                            )
                                        end
                                    end
                                )
                                mq.InputEnded:Connect(
                                    function(iq)
                                        if iq.UserInputType == Enum.UserInputType.MouseButton1 then
                                            if Aq then
                                                M = false
                                                Aq:Disconnect()
                                            end
                                        end
                                    end
                                )
                                X7.InputBegan:Connect(
                                    function(iq)
                                        if iq.UserInputType == Enum.UserInputType.MouseButton1 then
                                            if Fq then
                                                Fq:Disconnect()
                                            end
                                            M = true
                                            Fq =
                                                Oq.RenderStepped:Connect(
                                                function()
                                                    local Nq =
                                                        math.clamp(uq.Y - X7.AbsolutePosition.Y, 0, X7.AbsoluteSize.Y) /
                                                        X7.AbsoluteSize.Y
                                                    X7.Frame.Position = UDim2.fromScale(0, Nq)
                                                    p7 = 1 - Nq
                                                    zq()
                                                end
                                            )
                                        end
                                    end
                                )
                                X7.InputEnded:Connect(
                                    function(iq)
                                        if iq.UserInputType == Enum.UserInputType.MouseButton1 then
                                            if Fq then
                                                M = false
                                                Fq:Disconnect()
                                            end
                                        end
                                    end
                                )
                                pq.MouseButton1Click:Connect(
                                    function()
                                        t.ButtonEffect()
                                    end
                                )
                                aq.MouseButton1Click:Connect(
                                    function()
                                        t.ButtonEffect()
                                    end
                                )
                                tq.MouseButton1Click:Connect(
                                    function()
                                        t.ButtonEffect()
                                    end
                                )
                                local fq = false
                                local function dq()
                                    local iq, Nq = Eq.BackgroundColor3, kq.BackgroundColor3
                                    local Gq = UDim2.new(0, 0, 0, 0)
                                    local Qq = UDim2.new(0, 0, 1, 0)
                                    local Kq = Vector2.new(0, 1)
                                    game.TweenService:Create(
                                        jq,
                                        TweenInfo.new(getgenv().UIColor["Tween Animation 1 Speed"]),
                                        {Size = Gq, Position = Qq, AnchorPoint = Kq}
                                    ):Play()
                                end
                                spawn(
                                    function()
                                        dq()
                                    end
                                )
                            end
