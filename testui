                        Es.Parent = W
                        Es.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
                        Es.Size = UDim2.new(1, 0, 1, 0)
                        Es.Image = t.GetIMG(ls)
                    end
                    table.insert(
