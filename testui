                    local b7 = {}
                    local Z7 = -1.0
                    local x7 = -1.0
                    local H7 = 1
                    function b7.CreatePage(m7)
                        local c7 = tostring(m7.Page_Name)
                        local R7 = tostring(m7.Page_Title)
                        x7 = x7 + 1
                        Z7 = Z7 + 1
                        local B7 = Instance.new("Frame")
                        local _7 = Instance.new("Frame")
                        local q7 = Instance.new("UICorner")
                        local r7 = Instance.new("Frame")
                        local j7 = Instance.new("Frame")
