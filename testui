                                            function(kq)
                                                if kq.UserInputType == Enum.UserInputType.MouseButton1 then
                                                    local Dq =
                                                        Q7 and
                                                        tonumber(
                                                            string.format(
                                                                "%.1f",
                                                                (tonumber(G7) - tonumber(N7)) / U7 * Rq.AbsoluteSize.X +
                                                                    tonumber(N7)
                                                            )
                                                        ) or
                                                        math.floor(
                                                            (tonumber(G7) - tonumber(N7)) / U7 * Rq.AbsoluteSize.X +
                                                                tonumber(N7)
                                                        )
                                                    pcall(
                                                        function()
                                                            h7(Dq)
                                                            rq.Text = Dq
                                                        end
                                                    )
                                                    Rq.Size =
                                                        UDim2.new(
                                                        0,
                                                        math.clamp(jq.X - Rq.AbsolutePosition.X, 0, U7),
                                                        0,
                                                        6
                                                    )
                                                    moveconnection:Disconnect()
                                                    releaseconnection:Disconnect()
                                                end
                                            end
                                        )
                                    end
                                )
                                local function tq(Sq)
                                    if tonumber(Sq) <= N7 then
                                        Rq.Size = UDim2.new(0, (0 * U7), 0, 6)
                                        rq.Text = N7
                                        h7(tonumber(N7))
                                    elseif tonumber(Sq) >= G7 then
                                        Rq.Size = UDim2.new(0, (G7 / G7 * U7), 0, 6)
                                        rq.Text = G7
                                        h7(tonumber(G7))
                                    else
                                        Rq.Size = UDim2.new(1 - (G7 - Sq) / (G7 - N7), 0, 0, 6)
                                        h7(tonumber(Sq))
                                    end
                                end
                                rq.FocusLost:Connect(
                                    function()
                                        tq(rq.Text)
                                    end
                                )
                                local Jq = {}
                                function Jq.SetValue(Sq)
                                    tq(Sq)
                                end
                                return Jq
                            end
                            return L7
                        end
                        return X7
                    end
                    return b7
                end
                Library.CreateNoti(
                    {
                        Desc = "Hiro dz",
                        ShowTime = 120
                    }
                )
                --Library.CreateBox("Abc")
                k7.CreateBox("Abc")
