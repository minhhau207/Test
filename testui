                                    function()
                                        local L7 =
                                            a7 and
                                            tonumber(
                                                string.format(
                                                    "%.1f",
                                                    (tonumber(l7) - tonumber(E7)) / O7 * W7.AbsoluteSize.X +
                                                        tonumber(E7)
                                                )
                                            ) or
                                            math.floor(
                                                (tonumber(l7) - tonumber(E7)) / O7 * W7.AbsoluteSize.X + tonumber(E7)
                                            )
                                        pcall(
                                            function()
                                                V7(L7)
                                                z7.Text = L7
                                            end
                                        )
                                        W7.Size = UDim2.new(0, math.clamp(n7.X - W7.AbsolutePosition.X, 0, O7), 0, 6)
                                        moveconnection =
                                            n7.Move:Connect(
                                            function()
                                                local f7 =
                                                    a7 and
                                                    tonumber(
                                                        string.format(
                                                            "%.1f",
                                                            (tonumber(l7) - tonumber(E7)) / O7 * W7.AbsoluteSize.X +
                                                                tonumber(E7)
                                                        )
                                                    ) or
                                                    math.floor(
                                                        (tonumber(l7) - tonumber(E7)) / O7 * W7.AbsoluteSize.X +
                                                            tonumber(E7)
                                                    )
                                                pcall(
                                                    function()
                                                        V7(f7)
                                                        z7.Text = f7
                                                    end
                                                )
                                                W7.Size =
                                                    UDim2.new(0, math.clamp(n7.X - W7.AbsolutePosition.X, 0, O7), 0, 6)
                                            end
                                        )
                                        releaseconnection =
                                            D.InputEnded:Connect(
                                            function(f7)
                                                if f7.UserInputType == Enum.UserInputType.MouseButton1 then
                                                    local d7 =
                                                        a7 and
                                                        tonumber(
                                                            string.format(
                                                                "%.1f",
                                                                (tonumber(l7) - tonumber(E7)) / O7 * W7.AbsoluteSize.X +
                                                                    tonumber(E7)
                                                            )
                                                        ) or
                                                        math.floor(
                                                            (tonumber(l7) - tonumber(E7)) / O7 * W7.AbsoluteSize.X +
                                                                tonumber(E7)
                                                        )
                                                    pcall(
                                                        function()
                                                            V7(d7)
                                                            z7.Text = d7
                                                        end
                                                    )
                                                    W7.Size =
                                                        UDim2.new(
                                                        0,
                                                        math.clamp(n7.X - W7.AbsolutePosition.X, 0, O7),
                                                        0,
                                                        6
                                                    )
                                                    moveconnection:Disconnect()
                                                    releaseconnection:Disconnect()
                                                end
                                            end
                                        )
                                    end
                                )
                                local function P7(L7)
                                    if tonumber(L7) <= E7 then
                                        W7.Size = UDim2.new(0, (0 * O7), 0, 6)
                                        z7.Text = E7
                                        V7(tonumber(E7))
                                    elseif tonumber(L7) >= l7 then
                                        W7.Size = UDim2.new(0, (l7 / l7 * O7), 0, 6)
                                        z7.Text = l7
                                        V7(tonumber(l7))
                                    else
                                        W7.Size = UDim2.new(1 - (l7 - L7) / (l7 - E7), 0, 0, 6)
                                        V7(tonumber(L7))
                                    end
                                end
                                z7.FocusLost:Connect(
                                    function()
                                        P7(z7.Text)
                                    end
                                )
                                local e7 = {}
                                function e7.SetValue(L7)
                                    P7(L7)
                                end
                                return e7
                            end
                            return k7
                        end
                        return c7
                    end
