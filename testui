                function Library.CreateMain(O)
                    local A = tostring(O.Title) or "SIlver Hub"
                    getgenv().MainDesc = O.Desc or ""
                    local M = false
                    cac = false
                    local function o(m7, c7)
                        local R7 = nil
                        local B7 = nil
                        local _7 = nil
                        local q7 = nil
                        m7.InputBegan:Connect(
                            function(r7)
                                if
                                    r7.UserInputType == Enum.UserInputType.MouseButton1 or
                                        r7.UserInputType == Enum.UserInputType.Touch
                                 then
                                    R7 = true
                                    _7 = r7.Position
                                    q7 = c7.Position
                                    r7.Changed:Connect(
                                        function()
                                            if r7.UserInputState == Enum.UserInputState.End then
                                                R7 = false
                                            end
                                        end
                                    )
                                end
                            end
                        )
                        m7.InputChanged:Connect(
                            function(r7)
                                if
                                    r7.UserInputType == Enum.UserInputType.MouseMovement or
                                        r7.UserInputType == Enum.UserInputType.Touch
                                 then
                                    B7 = r7
                                end
                            end
                        )
                        D.InputChanged:Connect(
                            function(r7)
                                if r7 == B7 and R7 then
                                    local j7 = r7.Position - _7
                                    if not M and cac then
                                        J:Create(
                                            c7,
                                            TweenInfo.new(0.35, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
                                            {
                                                Position = UDim2.new(
                                                    q7.X.Scale,
                                                    q7.X.Offset + j7.X,
                                                    q7.Y.Scale,
                                                    q7.Y.Offset + j7.Y
                                                )
                                            }
                                        ):Play()
                                    elseif not M and not cac then
                                        c7.Position =
                                            UDim2.new(q7.X.Scale, q7.X.Offset + j7.X, q7.Y.Scale, q7.Y.Offset + j7.Y)
                                    end
                                end
                            end
                        )
                    end
                    local W = Instance.new("Frame")
                    local n = Instance.new("ImageLabel")
                    local e = Instance.new("UICorner")
                    local L = Instance.new("Frame")
                    local f = Instance.new("ImageLabel")
                    local K = Instance.new("TextLabel")
                    local h = Instance.new("Frame")
                    local bs = Instance.new("UICorner")
                    local Zs = Instance.new("ScrollingFrame")
                    local xs = Instance.new("UIListLayout")
                    local Hs = Instance.new("TextLabel")
                    local ms = Instance.new("Frame")
                    local cs = Instance.new("UIPageLayout")
                    local Rs = Instance.new("Frame")
                    local Bs = Instance.new("TextButton")
                    local _s = Instance.new("ImageLabel")
                    local qs = Instance.new("Frame")
                    local rs = Instance.new("Frame")
                    local js = Instance.new("Frame")
                    local ts = Instance.new("UIPageLayout")
                    local Js
                    W.Name = "Main"
                    W.Parent = Gui
                    W.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
                    W.BackgroundTransparency = 1.000
                    W.Position = UDim2.new(0.5, 0, 0.5, 0)
                    W.AnchorPoint = Vector2.new(0.5, 0.5)
                    W.Size = UDim2.new(0, 629, 0, 359)
                    o(W, W)
                    n.Name = "maingui"
                    n.Parent = W
                    n.AnchorPoint = Vector2.new(0.5, 0.5)
                    n.BackgroundColor3 = Color3.fromRGB(230, 230, 230)
                    n.BackgroundTransparency = 1.000
                    n.Position = UDim2.new(0.5, 0, 0.5, 0)
                    n.Selectable = true
                    n.Size = UDim2.new(1, 30, 1, 30)
                    n.Image = "rbxassetid://8068653048"
                    n.ScaleType = Enum.ScaleType.Slice
                    n.SliceCenter = Rect.new(15, 15, 175, 175)
                    n.SliceScale = 1.300
                    n.ImageColor3 = getgenv().UIColor["Border Color"]
                    table.insert(
                        _["Border Color"],
                        function()
                            n.ImageColor3 = getgenv().UIColor["Border Color"]
                        end
                    )
                    function t.ReloadMain(m7)
                        n.ImageColor3 = getgenv().UIColor["Title Text Color"]
                        local c7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[1])
                        local R7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[2])
                        local B7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[3])
                        local _7 = c7 .. "," .. R7 .. "," .. B7
                        K.Text = '<font color="rgb(' .. _7 .. ')">SIlver Hub</font> ' .. getgenv().MainDesc
                        table.insert(
                            _["Title Text Color"],
                            function()
                                n.ImageColor3 = getgenv().UIColor["Title Text Color"]
                                local r7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[1])
                                local j7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[2])
                                local t7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[3])
                                local J7 = r7 .. "," .. j7 .. "," .. t7
                                K.Text = '<font color="rgb(' .. J7 .. ')">SIlver Hub</font> ' .. getgenv().MainDesc
                            end
                        )
                        local q7
                        if
                            m7 ~= "" and type(m7) == "string" and string.find(m7:lower(), ".webm") and
                                pcall(
                                    function()
                                        writefile("seahub.webm", http_request({Url = m7}).Body)
                                    end
                                )
                         then
                            wait(.25)
                            local r7 = isfile("seahub.webm")
                            wait(.25)
                            if r7 then
                                q7 = Instance.new("VideoFrame")
                                q7.Name = "MainContainer"
                                q7.Parent = W
                                q7.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
                                q7.Size = UDim2.new(1, 0, 1, 0)
                                q7.Video = getsynasset("seahub.webm")
                                q7.Looped = true
                                q7:Play()
                                wait(.5)
                                delfile("seahub.webm")
                            end
                        else
                            q7 = Instance.new("ImageLabel")
                            q7.Name = "MainContainer"
                            q7.Parent = W
                            q7.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
                            q7.Size = UDim2.new(1, 0, 1, 0)
                            q7.Image = t.GetIMG(m7)
                        end
                        MainCorner_ = Instance.new("UICorner")
                        MainCorner_.CornerRadius = UDim.new(0, 4)
                        MainCorner_.Name = "MainCorner"
                        MainCorner_.Parent = q7
                        for r7, j7 in next, W:GetChildren() do
                            if j7.Name == "MainContainer" then
                                for t7, J7 in next, j7:GetChildren() do
                                    J7.Parent = q7
                                end
                                wait()
                                j7:Destroy()
                                break
                            end
                        end
                        table.insert(
                            _["Background 3 Color"],
                            function()
                                q7.BackgroundColor3 = getgenv().UIColor["Background 3 Color"]
                            end
                        )
                    end
                    n.ImageColor3 = getgenv().UIColor["Title Text Color"]
                    local Ss = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[1])
                    local ks = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[2])
                    local Ds = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[3])
                    local ps = Ss .. "," .. ks .. "," .. Ds
                    K.Text = '<font color="rgb(' .. ps .. ')">SIlver Hub</font> ' .. getgenv().MainDesc
                    table.insert(
                        _["Title Text Color"],
                        function()
                            n.ImageColor3 = getgenv().UIColor["Title Text Color"]
                            local m7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[1])
                            local c7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[2])
                            local R7 = tostring(t.Getcolor(getgenv().UIColor["Title Text Color"])[3])
                            local B7 = m7 .. "," .. c7 .. "," .. R7
                            K.Text = '<font color="rgb(' .. B7 .. ')">SIlver Hub</font> ' .. getgenv().MainDesc
                        end
                    )
                    local Es
                    local ls = getgenv().UIColor["Background Image"]
                    if
                        ls ~= "" and type(ls) == "string" and string.find(ls:lower(), ".webm") and
                            pcall(
                                function()
                                    writefile("seahub.webm", http_request({Url = ls}).Body)
                                end
                            )
                     then
                        wait(.25)
                        local m7 = isfile("seahub.webm")
                        wait(.25)
                        if m7 then
