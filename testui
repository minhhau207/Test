                                        mq.BackgroundColor3 = iq
                                        cq.Position = UDim2.new(iq and select(3, Color3.toHSV(iq)))
                                    end
                                    local Gq = 1 - Color3.toHSV(Nq)
                                    local Qq = X7.Frame.Position.Y.Scale
                                    if Qq ~= Gq and not ((Gq == 0 or Gq == 1) and (Qq == 1 or Qq == 0)) then
                                        X7.Frame.Position = UDim2.fromScale(0, Gq)
                                    end
                                    P7.Text, f7.Text, N7.Text = Xq(Nq)[1], Xq(Nq)[2], Xq(Nq)[3]
                                    Y7.BackgroundColor3 = Nq
                                    local Kq = {}
                                    getgenv().UIColor[V7] = Nq
                                end
                                local function nq(iq)
