                                if s7 then
                                    if s7 <= E7 then
                                        s7 = E7
                                    elseif s7 >= l7 then
                                        s7 = l7
                                    end
                                    W7.Size = UDim2.new(1 - (l7 - s7) / (l7 - E7), 0, 0, 6)
                                    z7.Text = s7
